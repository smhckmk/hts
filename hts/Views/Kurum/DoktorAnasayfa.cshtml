
@model IEnumerable<hts.Entity.DoktorTb>
@{
    ViewBag.Title = "DoktorAnasayfa";
    Layout = "/Views/Shared/KurumLayout.cshtml";
}

@Html.ActionLink("yeni ekle", "DoktorOlusturma")
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.doktorTc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.adSoyad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.maas)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.doktorTc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.adSoyad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.maas)
            </td>
            <td>
                @Html.ActionLink("duzenle", "DoktorDuzenle", new { id = item.doktorTc }) |
                @Html.ActionLink("sil", "DoktorSil", new { id = item.doktorTc })
            </td>
        </tr>
    }

</table>

