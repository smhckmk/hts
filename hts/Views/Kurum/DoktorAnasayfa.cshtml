
@model IEnumerable<hts.Entity.DoktorTb>
@{
    ViewBag.Title = "DoktorAnasayfa";
    Layout = "/Views/Shared/KurumLayout.cshtml";
}

@Html.ActionLink("Doktor Ekle", "DoktorOlusturma", new { did = ViewBag.id }, new { @class = "btn btn-success btn-xs" })
<table class="table mt-3" style="color:darkcyan">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.doktorTc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.adSoyad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.maas)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.doktorTc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.adSoyad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.maas)
            </td>
            <td>
                @Html.ActionLink("Düzenle", "DoktorDuzenle", new { id = item.doktorTc }, new { @class = "btn btn-warning btn-xs" })
                @Html.ActionLink("Sil", "DoktorSil", new { id = item.doktorTc }, new { @class = "btn btn-danger btn-xs" })
               
            </td>
        </tr>
    }

</table>

