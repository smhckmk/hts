@model IEnumerable < hts.Entity.BileklikTb >
    @{
    ViewBag.Title = "Bileklik";
}


<nav class="navbar navbar-expand-lg navbar-light">
    <div class="container">
        <a class="navbar-brand" href="/Doktor/DoktorAnasayfa">Anasayfa</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">

                <li class="nav-item">
                    <a class="nav-link" href="#">Bileklik</a>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Hastalar
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <a class="dropdown-item" href="#">Ozel Bilgi</a>
                        <a class="dropdown-item" href="#">Acil Durum</a>

                    </div>
                </li>

                <li class="nav-item">
                    <a class="nav-link" href="#">Cıkıs</a>
                </li>
            </ul>

            <form class="form-inline my-2 my-lg-0">
                <input class="form-control mr-sm-2" type="search" placeholder="bileklik no" aria-label="Search">
                <button class="btn btn-outline-success my-2 my-sm-0" type="submit">ara</button>
            </form>
            <ul navbar-nav mr-auto></ul>
        </div>
    </div>
</nav>
<div class="container mt-3">
    <div class="row">
        <div class="col-md-3">
            <img src="~/bitirme/img/avatar.png" alt="" class="rounded-circle" style="height:150px;;">
        </div>
        <div class="col-md-9">
            <h3>adsoyad</h3>
            <label>uzmanlık</label>
        </div>
    </div>
    <div class="mt-3">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th scope="col">Bileklik Id</th>
                    <th scope="col">imalat tarihi</th>
                    <th scope="col">verilebilme durumu</th>
                    
                </tr>
            </thead>
            <tbody>
                @foreach (var i in Model)
                {
                <tr>
                    <th>@i.Id</th>
                    <td>@i.imalatTarihi</td>
                    <td>@i.verilebilmeDurumu</td>
                    
                </tr>
                }
            </tbody>
        </table>

    </div>
</div>

